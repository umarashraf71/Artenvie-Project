
<div class="container">

<% if(error_msg != ''){ %>
    <div class="alert alert-danger alert-dismissible fade show text-center" style="margin-top: 100px !important; margin-bottom: -50px !important">
		<button type="button" class="close" data-dismiss="alert">&times;</button>
		<strong style="font-weight: 500; font-size: 18px;"> <%= error_msg %> </strong>
	</div>
    <% } %>
    
    <% if(success_msg != ''){ %>
        <div class="alert alert-success alert-dismissible fade show text-center" style="margin-top: 100px !important; margin-bottom: -50px !important">
            <button type="button" class="close" data-dismiss="alert">&times;</button>
            <strong style="font-weight: 500; font-size: 18px;"> <%= success_msg %> </strong>
        </div>
    <% } %>
    
    <% if(error != ''){ %>
    	
	<div class="alert alert-warning alert-dismissible fade show text-center" style="margin-top: 100px !important; margin-bottom: -50px !important">
		<button type="button" class="close" data-dismiss="alert">&times;</button>
		<strong style="font-weight: 500; font-size: 18px;"> <%= error %></strong>
	</div>
    <% } %>

</div>